createOtherNote note (note n){
	/* creates + returns new note*/
	double p = n -> pitch + 40;
	double d = n -> dur + 5;
	double v = n -> vol + 10;

	return note(p, v, d);

}

song score () {
	note n1 = note(440, 100, 5);
	note n2 = createOtherNote(n1);
	note n3 = createOtherNote(n2);
	chord c = chord(n1);
	c = c:n2; /* :n3;*/
	c = c:n3;
	/* c = c.n3;*/
	track t = track(26);
	t = t.c; 
	score s = score(t);
	return s;
}